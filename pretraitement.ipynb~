   "source": [
    "import spacy\n",
    "# Charge le petit pipeline français\n",
    "nlp = spacy.load(\"fr_core_news_md\")"
   ],
   "metadata": {
    "collapsed": false,
    "pycharm": {
     "name": "#%%\n"
    }
   }
  },
  {
   "cell_type": "code",
   "execution_count": 56,
   "outputs": [],
   "source": [
    "def cleanToken(sentence: str):\n",
    "        \"\"\"\n",
    "        Applique un pipeline de nettoyage à la phrase\n",
    "        :param sentences:\n",
    "        :type sentences: str\n",
    "        :return: sentence\n",
    "        :rtype: str\n",
    "        \"\"\"\n",
    "        # Instanciation du pipeline sur la phrase préalablement mise en miniscule\n",
    "        sentence.replace('.', '')\n",
    "        doc = nlp(sentence.lower())\n",
    "        allowed_postags = ['DET', 'PRON', 'ADP']\n",
    "\n",
    "        # return ' '.join(words)\n",
    "        return [\n",
    "            token.lemma_ for token in doc\n",
    "            if token.is_stop is not True and token.is_punct is not True\n",
    "            and token.tag_ not in allowed_postags and token.like_num is not True\n",
    "        ]\n",
    "\n",
    "def clearComment(dataframe):\n",
    "    dataframe['clear_comments'] = dataframe['comment']\n",
    "    dataframe['clear_comments'] = dataframe['comment'].apply(cleanToken)\n",
    "    return dataframe"
   ],
   "metadata": {
    "collapsed": false,
    "pycharm": {
     "name": "#%%\n"
    }
   }
  },
  {
   "cell_type": "code",
   "execution_count": 57,
   "outputs": [
    {
