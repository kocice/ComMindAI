    "def remainingDataFrame(df_in):\n",
    "    df_shape = df_in.shape[0]\n",
    "    df_out = None\n",
    "\n",
    "    for i in range(df_shape):\n",
    "        _id, product_title, product_url, comments = df_in['_id'][i], df_in['product_title'][i], \\\n",
    "                                                   df_in['product_url'][i], df_in['comments'][i]\n",
    "        comment_item_df = remaningProductComments(comment_item=comments)\n",
    "\n",
    "        comment_item_df = comment_item_df.assign(_id=_id)\n",
    "        comment_item_df = comment_item_df.assign(product_title=product_title)\n",
    "        comment_item_df = comment_item_df.assign(product_url=product_url)\n",
    "\n",
    "        if df_out is not None:\n",
    "            df_out = pd.concat([df_out, comment_item_df], axis=0, ignore_index=True)\n",
    "        else:\n",
    "            df_out = comment_item_df\n",
    "    return df_out.rename(columns={'body': 'comment'})"
   ],
